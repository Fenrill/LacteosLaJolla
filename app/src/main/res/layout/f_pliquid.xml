                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntBilletes6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />

                    <EditText
                        android:id="@+id/cntMonedas10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:inputType="number"
                        android:textAlignment="center" />


                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </LinearLayout>

</LinearLayout>